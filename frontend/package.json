{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:integration": "jest --testMatch='**/*.integration.test.{ts,tsx}'",
    "test:unit": "jest --testMatch='!**/*.integration.test.{ts,tsx}'"
  },
  "dependencies": {
    "ai": "^4.3.16",
    "next": "15.3.3",
    "react": "^18.0.0",
